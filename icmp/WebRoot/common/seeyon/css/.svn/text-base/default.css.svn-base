@CHARSET "UTF-8";
body, td, th, textarea, div, p {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000000;
}

body {
	margin: 6px;
}

a {
	font-size: 12px;
}
a.non-a {
	color: #000000;
	text-decoration: none;
}
a.non-a:HOVER {
	color: #000000;
	text-decoration: none;
}
.like-a{
	color: blue;
	text-decoration: underline;
	cursor: pointer;
}

form{
	margin: 0px;
}

.description-lable{
	color: green;
}

input, select{
	BACKGROUND-COLOR: #ffffff; BORDER-BOTTOM: #000000 1px solid; BORDER-LEFT: #000000 1px solid; BORDER-RIGHT: #000000 1px solid; BORDER-TOP: #000000 1px solid; FONT-SIZE: 9pt
}

.cursor-hand {
	cursor: pointer;
}

.bg-toolbar{
	background-color: #e9e9e9;
}

.separator{
	BORDER-RIGHT: #A4A4A4 1px solid; 
}

.back{
	width:70px;
	filter: Gray();
	padding-left:16px;
	cursor: pointer;
	background: url(../images/back_cursor.gif) left center no-repeat;
}
.back-cursor{
	width:70px;
	padding-left:16px;
	cursor: pointer;
	background: url(../images/back_cursor.gif) left center no-repeat;
}

.forward{
	width:70px;
	filter: Gray();
	padding-left:16px;
	cursor: pointer;
	background: url(../images/forward_cursor.gif) left center no-repeat;
}

.forward-cursor{
	width:70px;
	padding-left:16px;
	cursor: pointer;
	background: url(../images/forward_cursor.gif) left center no-repeat;
}

.refresh{
	width:70px;
	filter: Gray();
	padding-left:16px;
	cursor: pointer;
	background: url(../images/refresh_cursor.gif) left center no-repeat;
}

.refresh-cursor{
	width:70px;
	padding-left:16px;
	cursor: pointer;
	background: url(../images/refresh_cursor.gif) left center no-repeat;
}
