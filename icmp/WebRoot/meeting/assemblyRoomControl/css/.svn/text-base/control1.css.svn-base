@charset "utf-8";
/* CSS Document */

html,body{margin:0 0px; min-height:100%;_height:100%;font-style:normal;margin-top: 0px;}
body{ font-size:12px; font-family:Arial, Helvetica, sans-serif,"宋体";  color:#5B5B5B; text-align:center; background:#e5f7fe;}

a:link{color:#454f58;text-decoration:none;}
