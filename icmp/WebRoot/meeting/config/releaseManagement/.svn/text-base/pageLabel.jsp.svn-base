<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%>
 <script type="text/javascript">
	function switchTab(id){
		
		 if(id=="m1"){
		 	location.href="${sys_ctx }/meeting/config/releaseManagement/logoUplaod.jsp";
		 }else if(id=="m2"){
		 	location.href="${sys_ctx }/config/getSysConfigInfo.action";
		 }else if(id=="m3"){ 
		 	location.href="${sys_ctx }/meeting/config/releaseManagement/authorization.jsp";
		 }else if(id=="m4"){ 
		 	location.href="${sys_ctx }/dictionary/controlMenuList.action";
		 }else if(id=="m5"){ 
		 	location.href="${sys_ctx }/dictionary/controlRightMenuList.action";
		 }else if(id=="m6"){
		 	location.href="${sys_ctx }/meeting/config/releaseManagement/shouquanUplaod.jsp";
		 }
	}
</script>
        <div id="m">
        <!-- 
        	需要在每个页面设置选中页签状态
         -->
          <ul>
     		<li id="m1"  onclick="switchTab('m1')">logo上传</li> 
     		<li id="m2"  onclick="switchTab('m2')">系统名称</li>
     		<!--<li id="m3"  onclick="switchTab('m3')">授权</li>-->
     		<li id="m4"  onclick="switchTab('m4')">控制菜单</li> 
     		<li id="m5"  onclick="switchTab('m5')">右键菜单</li>
     		<li id="m6"  onclick="switchTab('m6')">软件授权</li>
          </ul>
        </div>