@charset utf-8
/*css*/
html,body{padding:0; margin:0; }
li{cursor:pointer;text-align: left;FONT-FAMILY:黑体;FONT-SIZE: 9pt;}

/*top*/
.pollTemplate{width:100%;border:1px solid #b4b4b4;height:30px;line-height:30px}
.pollTemplate span{width:190px;border-right:1px solid #b4b4b4;background:#eee;display:block;float:left;height:30px;text-align:right;padding-right:10px;margin-right:10px;}

/*button*/
.nextStepBtn {width: auto; margin: 0; font-weight: bold; color: #eee; background: #FB9337; border: 1px solid #F0882C; padding: 7px 10px; -moz-box-shadow: none; -webkit-box-shadow: none; box-shadow: none; cursor: pointer; -moz-border-radius: 2px; -webkit-border-radius: 2px;border-radius: 2px;margin:3px;}
.nextStepBtn:hover{ background: #485B79; border: 1px solid #3f526f; color: #fff;text-align:right}
.nextStepDiv{border:1px solid #b4b4b4;margin:0 auto;bottom:0;position:absolute;width:100%;text-align:right}
.callPollBtn{cursor:pointer;float: right}
.callPollDiv{margin: 2px 10px}

/*screenContainerDiv*/

.screenContainerDiv{width:100%;height:460px;overflow:auto;overflow-x:hidden;margin-top:2px}
.screenContainerDiv div ul.screenUl{margin:5px}
.screenContainerDivNoColor{width:539px;height:539px;border:0px solid #C0C0C0;margin-top: 2px;line-height:18px}
.participantsContainerDiv{width:100%;height:460px;border:0px solid #black;margin-top: 2px;line-height:18px;overflow:auto;overflow-x:hidden;margin-top:2px}
.participantsContainerDiv div ul{margin:5px}
.intervalTimeStyle{width:539px;height:20px;margin-top: 2px;}
.timeSpan{margin-left: 6px}

/*screenDiv*/
.screen{border:1px solid #b4b4b4;overflow: auto;float: left}
.participants{width:100%;height:100%;border:1px solid #b4b4b4;float: left;border-bottom:none}

/*participants*/
.participantsSpan{font-size: 16px;font-family: Arial,"黑体",Verdana,sans-serif ;FONT-SIZE: 9pt;}



.participantsDelBtn { padding:2px 7px; border: 1px solid #ccc; background: #eee url(../../../style/normal/images/buttons.png) repeat-x top left; color: #333;-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; cursor: pointer; opacity: 0.8;}
.participantsDelBtn:hover { text-decoration: none; opacity: 1; }




.addSelect{width: 140px;}

