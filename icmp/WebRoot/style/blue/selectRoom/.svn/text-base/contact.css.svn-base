/* CSS Document */
html, body, div, span, h1, h2, h3, h4, h5, h6, p,a, img, q, dl, dt, dd, ol, ul, li, form,table, tr, th, td {
	margin: 0;
	padding: 0;
}
html,body{ height:100%;overflow: auto; overflow-x: hidden; margin:0; padding:0; font-size:12px;}
ol,ul,li{list-style:none; margin:0; padding:0;}
img{ border:none;}
input{ vertical-align:middle; font-size:12px; padding:0px; color:#333333; margin:0px;}
.clear{
	clear: both;
	font-size: 1px;
	visibility: hidden;
}
.clear:after {clear: both;display: block;line-height: 0;font-size: 0;content: '.';visibility: hidden;}
.clearfix{	clear: both;}

