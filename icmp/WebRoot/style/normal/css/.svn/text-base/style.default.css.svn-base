	body.withvernav { background-position: -1px 0; }
	body.withmenucoll { background-position: 55px 0; }
	body.withmenucoll2 { background-position: 35px 0; }
	.imagelist li img { width: 240px; }	
}
@media screen and (max-width: 520px) {
