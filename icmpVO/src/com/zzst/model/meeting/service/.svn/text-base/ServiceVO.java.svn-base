package com.zzst.model.meeting.service;

import java.io.Serializable;

/**
 * class description: Service VO
 * 
 * @author ryan
 * @date Mon Aug 03 18:32:47 CST 2009
 */

public class ServiceVO implements Serializable {

	private static final long serialVersionUID = 1L;

	private Integer serviceID=Integer.MIN_VALUE;

	private Integer parentID =Integer.MIN_VALUE;
	
	//parent service name is related to parentID
	private String parentServiceName;
	
	private String serviceName;

	private String serviceUnit;

	private Float price=Float.MIN_VALUE;

	private String serviceDescription;

	private Integer orderNO=Integer.MIN_VALUE;
  
	private Long revision=Long.MIN_VALUE;

	private int state=Integer.MIN_VALUE;
	
	public int getState() {
		return state;
	}

	public void setState(int state) {
		this.state = state;
	}

	

	public void setServiceID(Integer serviceID) {
		this.serviceID = serviceID;
	}

	public Integer getServiceID() {
		return serviceID;
	}

	public void setServiceName(String serviceName) {
		this.serviceName = serviceName;
	}

	public String getServiceName() {
		return serviceName;
	}

	public void setServiceUnit(String serviceUnit) {
		this.serviceUnit = serviceUnit;
	}

	public String getServiceUnit() {
		return serviceUnit;
	}

	

	public void setServiceDescription(String serviceDescription) {
		this.serviceDescription = serviceDescription;
	}

	public String getServiceDescription() {
		return serviceDescription;
	}

	public void setOrderNO(Integer orderNO) {
		this.orderNO = orderNO;
	}

	public Integer getOrderNO() {
		return orderNO;
	}
 
	public Integer getParentID() {
		return parentID;
	}

	public void setParentID(Integer parentID) {
		this.parentID = parentID;
	}

	public void setRevision(Long revision) {
		this.revision = revision;
	}

	public Long getRevision() {
		return revision;
	}

	public String getParentServiceName() {
		return parentServiceName;
	}

	public void setParentServiceName(String parentServiceName) {
		this.parentServiceName = parentServiceName;
	}

	public Float getPrice() {
		return price;
	}

	public void setPrice(Float price) {
		this.price = price;
	}

}
