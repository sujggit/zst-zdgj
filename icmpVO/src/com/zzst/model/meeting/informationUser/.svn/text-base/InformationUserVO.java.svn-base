package com.zzst.model.meeting.informationUser;

import java.io.Serializable;
import java.sql.Timestamp;

/**
 * class description: InformationUser Object
 * @date Tue Jan 29 18:25:43 CST 2013
 * @author ryan
 */
public class InformationUserVO implements Serializable {

	private static final long serialVersionUID = 1L;	    



	private String infoID;
	private String disposeUserID;
	private Timestamp disposeTime;	            	
	private int status	=	Integer.MIN_VALUE;    
	private String description;




	public void setInfoID(String infoID){
		this.infoID = infoID;
	}
	public String getInfoID(){
		return infoID;
	}


	public void setDisposeUserID(String disposeUserID){
		this.disposeUserID = disposeUserID;
	}
	public String getDisposeUserID(){
		return disposeUserID;
	}


	public void setDisposeTime(Timestamp disposeTime){
		this.disposeTime = disposeTime;
	}
	public Timestamp getDisposeTime(){
		return disposeTime;
	}    	            	



	public void setStatus(int status){
		this.status = status;
	}                
	public int getStatus(){
		return status;
	}


	public void setDescription(String description){
		this.description = description;
	}
	public String getDescription(){
		return description;
	}





}


