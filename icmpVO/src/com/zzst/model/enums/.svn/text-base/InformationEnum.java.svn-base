package com.zzst.model.enums;

public class InformationEnum {
	public static final int 	TYPE_EQUIPMENT	=0;//设备告警信息
	public static final int 	TYPE_MEETING	=1;//会议结束前告警信息
	
	public static final int 	STATUS_INVALID		=	0;		//失效
	public static final int 	STATUS_VALID		=	1;		//有效
	
	//	消息推送状态
 	public static int scanStatus_no = 0;//未推送的消息
 	public static int scanStatus_yes  = 1;//已推送的消息

	public static String[][] getInfoType(){
 		String[][] status = new String[2][2];
 		status[0][0] = TYPE_EQUIPMENT+"";
 		status[0][1] = "设备告警";
 		status[1][0] = TYPE_MEETING+"";
 		status[1][1] = "会议结束提醒";
 		return status;
 	}
 	
}
